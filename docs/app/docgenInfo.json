  "src/components/Form/Form.js": {
    "props": {
      "children": {
        "type": {
          "name": "custom",
          "raw": "function(props, propName, componentName) {\n  let formButtons = [];\n\n  // Recursively search through all children of Form component and confirm\n  // at least one button exists, and at least one button is type submit.\n  function loopThruChildren(obj) {\n    Children.forEach(obj, (child) => {\n      // Check immediate children is type of button\n      if (_.has(child, 'type') && child.type === Button) {\n        formButtons.push(child.props.type);\n      }\n      // loop through child's children to find buttons\n      if (_.has(child, 'props.children') && _.isObject(child.props.children)) {\n        loopThruChildren(child.props.children);\n      }\n    });\n  }\n  loopThruChildren(props.children);\n\n  if (formButtons.length === 0 || !_.include(formButtons, 'submit')) {\n    return new Error('`Form` must have `Button` child with type of \"submit\".');\n  }\n}"
        },
        "required": false,
        "docBlock": {
          "description": "",
          "tags": []
        }
      },
      "onSubmit": {
        "type": {
          "name": "func"
        },
        "required": false,
        "docBlock": {
          "description": "",
          "tags": []
        }
      }
    },
    "docBlock": {
      "description": "",
      "tags": []
    }
  },
